{
	"buildCommand": "pnpm --filter @nadle/docs build",
	"outputDirectory": "packages/docs/build",
	"installCommand": "pnpm install",
	"framework": "docusaurus-2",
	"rewrites": [
		{
			"source": "/(.*)",
			"destination": "/packages/docs/$1"
		}
	],
	"github": {
		"silent": true,
		"autoJobCancelation": true
	}
}
