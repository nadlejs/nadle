{
	"compilerOptions": {
		"lib": ["ES2024"],
		"target": "ES2022",
		"moduleResolution": "NodeNext",
		"module": "NodeNext",
		"allowJs": true,
		"isolatedModules": true,
		"resolveJsonModule": true,
		"declaration": true,
		"sourceMap": true,
		"declarationMap": true,
		"strict": true,
		"skipLibCheck": true
	}
}
